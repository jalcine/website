---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title type="text" xml:lang="en">{{ site.description }}</title>
  <link type="application/atom+xml" href="http://jalcine.me/feed.xml" rel="self"/>
  <link type="text" href="http://blog.jalcine.me" rel="alternate"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ site.url }}/weblog</id>
  <author>
    <name>{{ site.author }}</name>
    <email>{{ site.email }}</email>
  </author>
  <rights>Copyright (c) 2012 - 2013, Jacky Alcine; all rights reserved.</rights>

  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link type="text/html" rel="alternative" href="{{ site.url }}/weblog{{ post.url }}"/>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>{{ site.url }}/weblog/{{ post.id }}</id>
    <author>
      <name>{{ site.author }}</name>
      <email>{{ site.email }}</email>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>
